${conf:misc}

[options]
; Server
admin_passwd = ${conf:admin_passwd}
addons_path = ${conf:addons_path}
csv_internal_sep = ,
import_partial = 
login_message = False
pidfile = None
reportgz = False
timezone = False
without_demo = False

; Database
db_host = ${conf:db_host}
db_port = ${conf:db_port}
db_user = ${conf:db_user}
db_password = ${conf:db_password}
db_name = ${conf:db_name}
db_template = ${conf:db_template}
db_maxconn = 64
list_db = True
pg_path = None
unaccent = False

; NET-RPC Protocol
netrpc = ${conf:netrpc}
netrpc_interface = ${conf:netrpc_interface}
netrpc_port = ${conf:netrpc_port}

; XML-RPC Protocol
xmlrpc = ${conf:netrpc}
xmlrpc_interface = ${conf:xmlrpc_interface}
xmlrpc_port = ${conf:xmlrpc_port}
proxy_mode = False

; Mail
smtp_server = ${conf:smtp_server}
smtp_port = ${conf:smtp_port}
smtp_ssl = False
smtp_user = False
smtp_password = False
email_from = False

; Logging
log_handler = ${conf:log_handler}
logfile = None
log_level = info
logrotate = True
debug_mode = False
syslog = False

; Testing
test_commit = False
test_disable = False
test_file = False
test_report_directory = False
assert_exit_level = error

; Performance
cache_timeout = 100000
cpu_time_limit = 60
max_cron_threads = 4
osv_memory_age_limit = 1.0
osv_memory_count_limit = False
virtual_memory_limit = 805306368
virtual_memory_reset = 671088640

; Unused options
;
; dbfilter = .*
; demo = {}
; server_wide_modules = None
; translate_modules = ['all']
;
; static_http_enable = False
; static_http_document_root = None
; static_http_url_prefix = None
;
; xmlrpcs = True
; xmlrpcs_interface = 
; xmlrpcs_port = 8071
; secure_cert_file = server.cert
; secure_pkey_file = server.pkey
